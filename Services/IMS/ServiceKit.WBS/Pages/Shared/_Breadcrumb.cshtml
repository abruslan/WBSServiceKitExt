@model BreadCrumbViewModel
<nav aria-label="breadcrumb" class="breadcrumb">
    <ol class="breadcrumb">
        <li class="breadcrumb-item" aria-current="page"><a asp-page="/Index">Главная</a></li>
        @foreach (var item in Model.Items)
        {
            <li class="breadcrumb-arrow"> -> </li>
            <li class="breadcrumb-item" aria-current="page"><a href="@item.Url">@item.Title</a></li>
        }
    </ol>
</nav>
